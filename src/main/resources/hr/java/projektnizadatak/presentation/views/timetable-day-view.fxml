<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.AnchorPane?>
<fx:root type="javafx.scene.layout.GridPane" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" vgap="4">
	<columnConstraints>
		<ColumnConstraints hgrow="ALWAYS"/>
	</columnConstraints>
	<rowConstraints>
		<RowConstraints minHeight="25.0" vgrow="NEVER"/>
		<RowConstraints vgrow="ALWAYS"/>
	</rowConstraints>

	<Label fx:id="titleLabel" GridPane.halignment="CENTER" style="-fx-font-size: 16px; -fx-padding: 16px 0 0 0;"/>

	<AnchorPane fx:id="contentAnchorPane" GridPane.rowIndex="1" styleClass="bounded-box"/>
</fx:root>
